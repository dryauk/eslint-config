{
  "name": "@dryauk/eslint-config",
  "version": "2.0.4",
  "description": "ESLint shareable configuration",
  "author": "DryAuk (https://github.com/dryauk)",
  "homepage": "https://github.com/dryauk/eslint-config#readme",
  "repository": "https://github.com/dryauk/eslint-config",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16",
    "yarn": ">=1.20"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "yarn lint:commit",
    "lint:commit": "commitlint --from=$( git rev-list --tags --max-count=1 )"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0"
  },
  "peerDependencies": {
    "eslint": "^8",
    "prettier": "^2",
    "typescript": "^4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@dryauk/commitlint-config": "^4.0.0",
    "@dryauk/semantic-release-config": "^3.0.0",
    "eslint": "^8.14.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "semantic-release": "^19.0.3",
    "typescript": "^5.1.3"
  },
  "commitlint": {
    "extends": "@dryauk/commitlint-config"
  },
  "release": {
    "extends": "@dryauk/semantic-release-config"
  }
}
