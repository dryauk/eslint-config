{
  "name": "@dryauk/eslint-config",
  "version": "1.3.0",
  "description": "ESLint shareable configuration",
  "author": "DryAuk (https://github.com/dryauk)",
  "homepage": "https://github.com/dryauk/eslint-config#readme",
  "repository": "https://github.com/dryauk/eslint-config",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=16",
    "yarn": ">=1.20"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "yarn lint:commit",
    "lint:commit": "commitlint --from=$( git rev-list --tags --max-count=1 )"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3"
  },
  "peerDependencies": {
    "eslint": "^7",
    "prettier": "^2",
    "typescript": "^4"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@dryauk/commitlint-config": "^2.0.0",
    "@dryauk/semantic-release-config": "^2.0.0",
    "husky": "^7.0.4",
    "semantic-release": "^18.0.1"
  },
  "commitlint": {
    "extends": "@dryauk/commitlint-config"
  },
  "release": {
    "extends": "@dryauk/semantic-release-config"
  }
}
